,CAAA;AAAA,QAAA,IAAA,EAAA,CAAA;AAAA,QAAA,OAAA,EAAA,KAAA,OAAA;AAAA,QAAA,QAAA,EAAA,KAAA,QAAA;AAAA,QAAA,QAAA,EAAA,KAAA,QAAA;AAAA,QAAA,OAAA,EAAA,KAAA,OAAA;AAAA,QAAA,QAAA,EAAA,KAAA;AAAA,OAAA;AACA,KALA;AAMA,IAAA,iBANA,6BAMA,CANA,EAMA;AACA,UAAA,OAAA,GAAA,CAAA,CAAA,MAAA,CAAA,OAAA,CAAA,CAAA,CAAA,MAAA,CAAA,OAAA,CAAA,aAAA,EAAA,SAAA;AACA,WAAA,QAAA,GAAA,OAAA;AACA,KATA;AAUA,IAAA,iBAVA,6BAUA,CAVA,EAUA;AACA,UAAA,OAAA,GAAA,CAAA,CAAA,MAAA,CAAA,OAAA,CAAA,CAAA,CAAA,MAAA,CAAA,OAAA,CAAA,aAAA,EAAA,SAAA;AACA,WAAA,QAAA,GAAA,OAAA;AACA;AAbA;AArCA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <h3>Add Post</h3>\r\n        <div class=\"add\">\r\n            Location:\r\n                    <select @change=\"setLocationOption($event)\">\r\n                        <option value=\"Location_None\">{{location}}</option>\r\n                        <option v-for=\"location in Locations\" :key=\"location.id\" value:location.place>{{ location.place }}</option>\r\n                    </select>\r\n                    Category:\r\n                    <select @change=\"setCategoryOption($event)\">\r\n                        <option value=\"Category_None\">{{category}}</option>\r\n                        <option v-for=\"category in Categories\" :key=\"category.id\" value:category.cat>{{ category.cat }}</option>\r\n                    </select>\r\n            <form @submit=\"onSubmit\">\r\n                <input type=\"text\" v-model=\"content\" placeholder=\"Add text...\">\r\n                <input type=\"submit\" value=\"Submit\">\r\n            </form>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapActions } from 'vuex';\r\n\r\nexport default {\r\n    name: \"AddPost\",\r\n    data() {\r\n        return {\r\n            content: \"\",\r\n            username: \"Anonymous\",\r\n            location: \"\",\r\n            category: \"\",\r\n            groupID: -1,\r\n            Locations: [{\r\n                id: 1,\r\n                place: \"Cape Town\"\r\n            },\r\n            {\r\n                id: 2,\r\n                place: \"Port Elizabeth\"\r\n            },\r\n            {\r\n                id: 3,\r\n                place: \"Kimberley\"\r\n            }\r\n            ],\r\n            Categories: [{\r\n                id: 1,\r\n                cat: \"Photos\"\r\n            },\r\n            {\r\n                id: 2,\r\n                cat: \"Reviews\"\r\n            },\r\n            {\r\n                id: 3,\r\n                cat: \"Sports\"\r\n            }\r\n            ]\r\n        }\r\n    },\r\n    methods: {\r\n        ...mapActions(['addPost']),\r\n        onSubmit(e) {\r\n            e.preventDefault();\r\n            this.addPost({user: 1, content: this.content, location: this.location, category: this.category, groupID: this.groupID, username: this.username});\r\n        },\r\n        setLocationOption(e) {\r\n            const filtOpt = e.target.options[e.target.options.selectedIndex].innerText;\r\n            this.location = filtOpt\r\n        },\r\n        setCategoryOption(e) {\r\n            const filtOpt = e.target.options[e.target.options.selectedIndex].innerText;\r\n            this.category = filtOpt\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\nform {\r\n    display: flex;\r\n}\r\n\r\ninput[type=\"text\"] {\r\n    flex: 10;\r\n    padding: 10px;\r\n    border: 1px solid #ccc;\r\n    outline: 0;\r\n}\r\n\r\ninput[type=\"submit\"] {\r\n    flex: 2;\r\n    background: #2c3e50;\r\n    color: #fff;\r\n    border: 1px #41b883 solid;\r\n    cursor: pointer;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}